@model IEnumerable<User>

<div class="container">
    <table class='table table-striped'>
        <tr>
            <th>UserId</th>
            <th>Username</th>
            <th>Password</th>

        </tr>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserId</td>
                <td>@user.UserName</td>
                <td>@user.Password</td>
            </tr>
        }
    </table>
</div> 